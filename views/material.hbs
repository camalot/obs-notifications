<!doctype html>
<html>

<head>
	<title>:: {{BOTINFO.TITLE}} :: {{title}}</title>
	<link rel="stylesheet" href="//fonts.googleapis.com/icon?family=Material+Icons">
	<link rel="stylesheet" href="//code.getmdl.io/1.3.0/material.deep_purple-indigo.min.css">
	<link rel="stylesheet" href="//fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en">
	<link href="//cdn.materialdesignicons.com/2.6.95/css/materialdesignicons.min.css" media="all" rel="stylesheet" type="text/css"
	/>
	<link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" type="text/css" />
	<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
	<script defer src="//code.getmdl.io/1.3.0/material.min.js"></script>
	<script defer src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

{{!-- 
	<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
	<link rel="manifest" href="/site.webmanifest">
	<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5"> 
	<meta name="msapplication-TileColor" content="#da532c">
	<meta name="theme-color" content="#ffffff">

	<link rel="stylesheet" href="/assets/css/style.css" /> --}}
	{{{block 'head'}}}
</head>

<body>
	{{!-- <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
		<div class="android-header mdl-layout__header mdl-layout__header--waterfall mdl-color--blue-400">
			<div class="mdl-layout__header-row">
				<h3>
					<img src="/assets/images/bit13-48.png" style="vertical-align: text-bottom" />{{BOTINFO.TITLE}} :: {{title}}</h3>
				<div class="mdl-layout-spacer"></div>
				{{#if user}}
				<a class="mdl-button mdl-js-button mdl-button--icon" href="/me">
					<img src="{{user._json.avatar_url}}" style="height: 32px; width: 32px;" />
				</a>
				<a class="mdl-button mdl-js-button mdl-button--icon" href="/logout" title="logout">
					<i class="mdi mdi-logout" style="width: 16px">logout</i>
				</a>
				{{else}}
				<a class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent mdl-color--indigo-50 mdl-color-text--black"
				 href="/login" title="login">Login</a>&nbsp; {{/if}}
			</div>
		</div> --}}

		<main class="mdl-layout__content mdl-color--grey-100 ">
			{{{body}}}
		</main>
		<footer class="mdl-mini-footer ">
			<div class="mdl-mini-footer__left-section ">
				<div class="mdl-logo "></div>
				<ul class="mdl-mini-footer__link-list ">
					<li>
						<a href="{{BOTINFO.URL}} " target="_blank ">{{BOTINFO.TITLE}} v{{BOTINFO.VERSION}}</a>
					</li>
					<li>
						<a href="# ">Privacy &amp; Terms</a>
					</li>
				</ul>
			</div>
		</footer>
	</div>
	{{#if message}}
	<script>
		$(function () {
			toastr.options = {
				"closeButton ": true,
				"debug ": false,
				"newestOnTop ": false,
				"progressBar ": false,
				"positionClass ": "toast-top-center ",
				"preventDuplicates ": true,
				"showDuration ": "300 ",
				"hideDuration ": "5000 ",
				"timeOut ": "-1 ",
				"extendedTimeOut ": "1000 ",
				"showEasing ": "swing ",
				"hideEasing ": "linear ",
				"showMethod ": "fadeIn ",
				"hideMethod ": "fadeOut "
			};
			{ { #each message } }
			toastr["error "]("{{this}} ");
			{
				{
					/each}}
				});
	</script> {{/if}}{{{block 'scripts'}}}
</body>

</html>
